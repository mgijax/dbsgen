  <target name="install.templates" description="Puts template files in classes directory"
          depends="compile">
    <copy todir="${classes.dir}/org/jax/mgi/shr/dbutils/cg">
      <fileset dir="${src.dir}/org/jax/mgi/shr/dbutils/cg">
         <include name="**/*.vm"/>
      </fileset>
    </copy>
  </target>

  <target name="buildall" description="Runs the targets 'compile', 'javadocs', 'install.templates' and 'jar'"
          depends="compile, javadocs, install.templates, jar">
  </target>
